{
	"name": "vue-cli-mult",
	"version": "0.0.1",
	"description": "A simple CLI for scaffolding Vue.js projects.",
	"author": "zhangkaiqiang <zhangkaiqiang@58ganji.com>",
	"keywords": [
		"vue",
		"cli",
		"multiple pages"
	],
	"private": true,
	"scripts": {
		"build-all": "npm run build -- home && npm run build -- userEntry && npm run build -- userCenter && npm run build -- coins && npm run build -- pawn && npm run build -- about && npm run build -- transaction && npm run build -- activity",
		"dev": "node build/dev-server.js",
		"dev:xunit": "node build/dev-server.js -- --reporter xunit",
		"start:": "node build/dev-server.js",
		"start:xunit": "node build/dev-server.js -- --reporter xunit",
		"build": "node build/build.js",
		"build:xunit": "node build/build.js -- --reporter xunit",
		"build:dll": "webpack --config build/webpack.dll.conf.js",
		"unit": "cross-env BABEL_ENV=test karma start test/unit/karma.conf.js --single-run",
		"e2e": "node test/e2e/runner.js",
		"test": "npm run unit && npm run e2e",
		"lint": "eslint --ext .js,.vue src test/unit/specs test/e2e/specs"
	},
